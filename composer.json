{
    "name": "acquia/blt-project",
    "description": "Project template for Drupal 8 sites built with BLT.",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "FrDH/jQuery.mmenu",
                "type": "drupal-library",
                "version": "6.1.8",
                "dist": {
                    "url": "https://github.com/FrDH/jQuery.mmenu/archive/v6.1.8.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "joeldbirch/superfish",
                "type": "drupal-library",
                "version": "1.7.9",
                "dist": {
                    "url": "https://github.com/joeldbirch/superfish/archive/v1.7.9.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "hammerjs/hammer.js",
                "type": "drupal-library",
                "version": "2.0.8",
                "dist": {
                    "url": "https://github.com/hammerjs/hammer.js/archive/v2.0.8.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "acquia/blt": "^8.3",
        "drupal/metatag": "^1.2",
        "drupal/redirect": "^1.0@beta",
        "drupal/coffee": "^1.0@beta",
        "drupal/adminimal_theme": "^1.3",
        "drupal/adminimal_admin_toolbar": "^1.5",
        "drupal/auto_entitylabel": "^2.0@beta",
        "drupal/features": "^3.5",
        "drupal/field_collection": "^1.0@alpha",
        "drupal/field_formatter_class": "^1.0",
        "drupal/field_group": "^1.0@RC",
        "drupal/file_browser": "^1.1",
        "drupal/google_analytics": "^2.1",
        "drupal/imce": "^1.6",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/media_entity": "^1.6",
        "drupal/media_entity_image": "^1.2",
        "drupal/media_entity_video": "^1.0@beta",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.0",
        "drupal/rules": "^3.0@alpha",
        "drupal/responsive_menu": "^2.4",
        "drupal/rewrite_field": "^1.0",
        "drupal/simple_block": "^1.0@beta",
        "drupal/twig_field_value": "^1.1",
        "drupal/twig_tweak": "^1.9",
        "drupal/slick": "^1.0",
        "drupal/webform": "^5.0@beta",
        "drupal/focal_point": "^1.0@beta",
        "drupal/blazy": "^1.0@RC",
        "drupal/extlink": "^1.0",
        "drupal/page_manager": "^4.0@beta",
        "drupal/block_class": "^1.0@alpha",
        "drupal/config_direct_save": "^1.0",
        "drupal/slick_views": "^1.0@RC",
        "drupal/slick_extras": "^1.0@beta",
        "drupal/simple_sitemap": "^2.10",
        "drupal/yoast_seo": "^1.3",
        "drupal/recreate_block_content": "^1.0",
        "drupal/context": "^4.0@beta",
        "drush/drush": "^8.1",
        "drupal/console": "~1.0",
        "FrDH/jQuery.mmenu": "^6.1",
        "hammerjs/hammer.js": "^2.0",
        "drupal/panelizer": "^4.0",
        "joeldbirch/superfish": "^1.7",
        "drupal/we_megamenu": "^1.0@RC",
        "drupal/weight": "^3.0",
        "drupal/shield": "^1.1"

    },
    "require-dev": {
        "geerlingguy/drupal-vm": "^4.6"
    },
    "config": {
        "platform": {"php":"5.6"}
    },
    "extra": {
        "enable-patching": true,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/libraries/mmenu": [
                "FrDH/jQuery.mmenu"
            ],
            "docroot/libraries/hammerjs": [
                "hammerjs/hammer.js"
            ]
        },
        "merge-plugin": {
            "require": [
                "vendor/acquia/blt/composer.required.json",
                "vendor/acquia/blt/composer.suggested.json"
            ],
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true,
            "replace": false,
            "ignore-duplicates": true
        }
    }
}